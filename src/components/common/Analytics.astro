---
import { ANALYTICS } from 'astrowind:config';

const idString = "https://www.googletagmanager.com/gtag/js?id="+ANALYTICS.vendors.googleAnalytics.id;
const id = ANALYTICS.vendors.googleAnalytics.id;
console.log(id);

---


<script type="text/partytown" src={String(idString)}></script>
<script type="text/partytown">
  window.dataLayer = window.dataLayer || [];
  window.gtag = function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', id);
</script>
